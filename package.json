{
  "name": "simple_crud_api",
  "version": "1.0.0",
  "description": "simple crud api based on memory save",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start:dev": "cross-env MULTI=false nodemon --legacy-watch src/index.js",
    "start:multi": "cross-env MULTI=true tsnd --respawn src/index.js",
    "start:prod": "webpack && node build/main-bundle.js",
    "start:prod:port-cross-env": "webpack && cross-env PORT=7000 node build/main-bundle.js",
    "build": "webpack",
    "test": "cross-env PORT=3000 jest --silent",
    "start": "node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orifjonov-abbosbek/simple_CRUD_API.git"
  },
  "author": "Orifjonov Abbobek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/orifjonov-abbosbek/simple_CRUD_API/issues"
  },
  "homepage": "https://github.com/orifjonov-abbosbek/simple_CRUD_API#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
